<p id="notice"><%= notice %></p>
<div class="container">
  <p>
    <strong>Date:</strong>
    <%= @post.created_at.to_date %>
  </p>
  
  <p>
    <strong>Title:</strong>
    <%= @post.title %>
  </p>
  
  <p>
    <strong>Body:</strong>
    <%= @post.body %>
  </p>
</div>

<%= link_to 'Edit', edit_post_path(@post), class: "btn btn-default" %> |
<%= link_to 'Back', posts_path, class: "btn btn-default" %>
